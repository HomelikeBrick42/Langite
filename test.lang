const True: Type = struct {}
const False = struct {}
const Bool = enum {
    true: True,
    false: False,
}

const Foo = struct {}
const Bar = struct {}

// type annotation is optional
const foo: fn(T: Type) -> Type = fn(T: Type) -> Type {
    match T {
        Foo => Bar,
        _ => T,
    }
}

const bar = fn(const T: Type, const U: Type, f: fn(x: T) -> U, x: T) -> U {
    f(x)
}
